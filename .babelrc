{
  "env": {
    "production": {
      "plugins": [
        [
          "transform-remove-console",
          {
            "exclude": [
              "error",
              "warn"
            ]
          }
        ]
      ]
    }
  },
  "presets": [
    "stage-0",
    "env",
    "react"
  ],
  "plugins": [
    "add-module-exports",
    "transform-runtime",
    "syntax-dynamic-import",
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": true
      }
    ],
    "transform-class-properties"
  ]
}